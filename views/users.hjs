<!DOCTYPE html>
<html>
  <head>
    <title>{{ title }}</title>
    <link rel='stylesheet' href='/stylesheets/style.css' />
    <script src="https://code.jquery.com/jquery-3.4.1.min.js" integrity="sha256-CSXorXvZcTkaix6Yvo6HppcZGetbYMGWSFlBw8HfCJo=" crossorigin="anonymous"></script>
    <script type="text/javascript">
      $(function (){
        window.WebSocket = window.WebSocket || window.MozWebSocket;
        var connection = new WebSocket('wss://localhost:8002');

        connection.onopen = function () {
          console.log('Connection Opened');
          connection.send('GetUserCount');
        };

        connection.onerror = function (error) {
          console.log('Failed to open connection error : ' + error);
        };

        connection.onmessage = function (message) {
        try {
          var json = JSON.parse(message.data);
          console.log(JSON.stringify(json));
          console.log(json.data.userCount);
          document.getElementById("span_UserCount").textContent=json.data.userCount;
        } catch (e) {
          console.log('This doesn\'t look like a valid JSON: ',message.data);
          return;
          }
        };
      });
    </script>
  </head>
   <div style='border-bottom: 5px solid #084e8a;'>
     <div class='containerNewUI'>
       <div class='containerBlock'>
          <a href='#' id='headerLink_New' onclick='javascript:parent.ShowScreenHelper_OpenInTabOrPopUp('http://www.cubussolutions.com',screen.height,screen.width)' title='CUBUS Logo' class='logo'>
            <img id='ctl00_ctl01_imgLogo_New' class='primarylogo' src='/images/logo.png' alt='CUBUS Logo' style='border-width:0px;'>
          </a>
          <div style='float:right'>
            {{!-- <input type='button' style='margin-top:50%;' value='Logout' onclick='window.open("./")'></input> --}}
            <a href="./">
              <img src="/images/logout.svg" style="width:25px;height:25px;margin-top:50%"/>
            </a>
          </div>
        </div>
        <span class='containerNewUI' style='text-align: left;display: inline-block;padding-bottom: 2px;width: 50%;'>Welcome {{Name}}</span>
        <span class="containerNewUI" style="float:right;width: 50%;text-align: right;">
          Users : 
          <span id="span_UserCount">1</span>
        </span>
      </div>
    </div>
  <body>
      {{{userInfo}}} 
      {{{AcctInfo}}} 
  </body>
</html>
